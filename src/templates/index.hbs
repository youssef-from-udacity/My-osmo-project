<!DOCTYPE html>
<html>
    <head>
        <title>Bootstrap 101 Template</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="node_modules\bootstrap3\dist\css\bootstrap.min.css" media="screen">
        <link rel="stylesheet" href="dist\styles\main.css" media="screen">
            <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
            <!--[if lt IE 9]>
            <script src="js/html5shiv.js"></script>
            <script src="js/respond.min.js"></script>
            <![endif]-->
        </head>
        <body>
            <div class="wrapper">
                {{> component/01-sidebar/hbs/sidebar}}
                <div class="content">
                    {{> component/02-header/hbs/header}}
                    <div class="container-fluid">
                        <div class="row fluid-block">
                            {{> component/03-main/1-caption/hbs/caption}}
                            {{> component/03-main/2-carousel/hbs/carousel}}
                            {{> component/03-main/3-video/hbs/video}}
                            {{> component/03-main/4-modal/hbs/modal}}
                        </div>
                        <div class="row">
                            <div class="osmoContainer">
                                {{> component/04-description/hbs/description}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/05-homeStrip/hbs/homeStrip}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/06-coding-family/hbs/coding-family}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/07-hotWeels/hbs/hotWeels}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/08-tangram/hbs/tangram}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/09-pizza/hbs/pizza}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/10-monster/hbs/monster}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/11-numbers/hbs/numbers}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/12-masterpiece/hbs/masterpiece}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/13-words/hbs/words}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/14-nowton/hbs/nowton}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            {{> component/15-awardWining/hbs/awardWining}}
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/16-osmoSchools/hbs/osmoSchools}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            {{> component/17-workStrip/hbs/workStrip}}
                        </div>
                        <div class="row lazyload">
                            <div class="osmoContainer">
                                {{> component/18-osmOrder/hbs/osmOrder}}
                            </div>
                        </div>
                        <div class="row lazyload">
                            {{> component/19-bonus/hbs/bonus}}
                        </div>
                        <div class="row lazyload">
                            {{> component/20-footer/hbs/footer}}
                        </div>
                    </div>
                </div>
            </div>
            <!--          jQuery (necessary for Bootstrap's JavaScript plugins) -->
            <script src="node_modules\jquery\dist\jquery.min.js"></script>
            <!-- Include all compiled plugins (below), or include individual files as needed -->
            <script src="node_modules\bootstrap3\dist\js\bootstrap.min.js" async></script>
            <script src="dist\scripts\main.js" defer></script>
        </body>
    </html>
